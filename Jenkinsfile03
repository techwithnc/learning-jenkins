pipeline{
    agent any
    stages{
        stage("build"){
            steps{
                echo " This stage is Building...."
                sh 'cat file.txt'
            }
        }
        stage("test"){
            steps{
                echo "This stage is Testing ... "
            }
        }
        stage("deploy"){
            steps{
                echo " This stage is Deploying"
            }
        }
    }
}